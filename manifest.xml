<package>
    <description brief="Python library for interacting with HERB.">
        Python library for interacting with HERB through OpenRAVE. HERB is a
        bimanual mobile manipulator developed by the Personal Robotics Lab at
        Carnegie Mellon University.
    </description>
    <author>Michael Koval, Pras Velagapudi</author>
    <license>BSD</license>

    <depend package="rospy"/>
    <depend package="rospack"/>
    <!-- OpenRAVE -->
    <depend package="openrave"/>
    <depend package="openrave_exports"/>
    <depend package="manipulation2"/>
    <depend package="pr_ordata"/>
    <depend package="prrave"/>
    <!-- hardware plugins -->
    <depend package="or_ros_module"/>
    <depend package="or_moped_sensorsystem"/>
    <depend package="or_multi_controller"/>
    <depend package="or_owd_controller"/>
    <depend package="or_segway_controller"/>
    <depend package="or_barrett_ft_sensor"/>
    <depend package="or_talker_module"/>
    <!-- planner plugins -->
    <depend package="cbirrt2"/>
    <depend package="orcdchomp"/>
</package>
